{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-empty": 0,
        "no-irregular-whitespace": 0,
        "import/prefer-default-export": "off",
        "indent": [
            2,
            2
        ],
        "react/jsx-indent": [
            "error",
            2
        ],
        "no-unused-vars": "off"

    }
}